<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", admin_admins_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_admin_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.show", :default => "Show")}", admin_admin_path %></li>
    </ul>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b><%= t("activerecord.attributes.admin_admin.email", :default => t("activerecord.labels.email", :default => "Email")) %>:</b>
          <%= @admin.email %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.reset_password_token", :default => t("activerecord.labels.reset_password_token", :default => "Reset password token")) %>:</b>
          <%= @admin.reset_password_token %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.remember_token", :default => t("activerecord.labels.remember_token", :default => "Remember token")) %>:</b>
          <%= @admin.remember_token %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.remember_created_at", :default => t("activerecord.labels.remember_created_at", :default => "Remember created at")) %>:</b>
          <%= @admin.remember_created_at %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.sign_in_count", :default => t("activerecord.labels.sign_in_count", :default => "Sign in count")) %>:</b>
          <%= @admin.sign_in_count %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.current_sign_in_at", :default => t("activerecord.labels.current_sign_in_at", :default => "Current sign in at")) %>:</b>
          <%= @admin.current_sign_in_at %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.last_sign_in_at", :default => t("activerecord.labels.last_sign_in_at", :default => "Last sign in at")) %>:</b>
          <%= @admin.last_sign_in_at %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.current_sign_in_ip", :default => t("activerecord.labels.current_sign_in_ip", :default => "Current sign in ip")) %>:</b>
          <%= @admin.current_sign_in_ip %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.last_sign_in_ip", :default => t("activerecord.labels.last_sign_in_ip", :default => "Last sign in ip")) %>:</b>
          <%= @admin.last_sign_in_ip %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.failed_attempts", :default => t("activerecord.labels.failed_attempts", :default => "Failed attempts")) %>:</b>
          <%= @admin.failed_attempts %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.unlock_token", :default => t("activerecord.labels.unlock_token", :default => "Unlock token")) %>:</b>
          <%= @admin.unlock_token %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.admin_admin.locked_at", :default => t("activerecord.labels.locked_at", :default => "Locked at")) %>:</b>
          <%= @admin.locked_at %>
        </p>
      <div class="wat-cf">
        <%= link_to image_tag("web-app-theme/icons/application_edit.png", :alt => "#{t("web-app-theme.edit", :default=> "Edit")}") + " " + t("web-app-theme.edit", :default=> "Edit"), edit_admin_admin_path(@admin), :class => "button" %> 
        <%= link_to image_tag("web-app-theme/icons/cross.png", :alt => "#{t("web-app-theme.delete", :default=> "Delete")}") + " " + t("web-app-theme.delete", :default => "Delete"), admin_admin_path(@admin), :method => "delete", :class => "button", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
      </div>      
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>