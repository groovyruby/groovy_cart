<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", admin_shipping_methods_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_shipping_method_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> ShippingMethods</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.shipping_method.name", :default => t("activerecord.labels.name", :default => "Name")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @shipping_methods.each do |shipping_method| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= shipping_method.id %>
          </td>
                    <td>
            <%= link_to shipping_method.name, admin_shipping_method_path(shipping_method) %>
          </td>
                    <td>
            <%= shipping_method.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", admin_shipping_method_path(shipping_method) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admin_shipping_method_path(shipping_method) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", admin_shipping_method_path(shipping_method), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
